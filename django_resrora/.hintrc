{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "no-inline-styles": "off",
    "axe/structure": "off",
    "axe/forms": "off",
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[referrerpolicy=no-referrer-when-downgrade]"
        ]
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-moz-appearance",
          "min-height: auto"
        ]
      }
    ]
  }
}